{
  "name": "patron",
  "version": "4.0.0",
  "author": "ElJay#7711",
  "license": "GPL-3.0-or-later",
  "description": "The cleanest command framework for discord.js and eris.",
  "keywords": [
    "discord",
    "api",
    "nodejs",
    "discordjs",
    "eris",
    "command",
    "framework"
  ],
  "main": "./lib/index.js",
  "exports": {
    ".": [
      {
        "require": "./lib/index.js",
        "import": "./lib/esm.mjs"
      },
      "./lib/index.js"
    ],
    "./esm": "./lib/esm.mjs"
  },
  "types": "./types/index.d.ts",
  "scripts": {
    "preinstall": "node lib/preinstall.js",
    "postinstall": "node lib/postinstall.js",
    "lint": "eslint lib --ext .js",
    "test": "npm run lint"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^13.13.1",
    "eslint": "^6.8.0",
    "typescript": "^3.8.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LJNeon/patron.js.git"
  },
  "bugs": {
    "url": "https://github.com/LJNeon/patron.js/issues"
  },
  "homepage": "https://github.com/LJNeon/patron.js#readme"
